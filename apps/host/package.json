    "react-dom": "^18.2.0",
    "react-router-dom": "^5.2.0",
    "@libs/theme": "workspace:^"
  },
  "devDependencies": {
    "@rsbuild/core": "^0.6.3",
    "@rsbuild/plugin-react": "^0.6.3",
    "@types/history": "^4.7.11",
